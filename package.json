{
  "name": "notion-custom-domain",
  "version": "0.1.0",
  "description": "Custom Domains for Notion",
  "license": "MIT",
  "author": "E. Hosso",
  "scripts": {
    "debug": "node --nolazy --inspect-brk -r ts-node/register src/index.ts",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "develop": "vercel dev",
    "postinstall": "ts-node scripts/setup-routes.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix && prettier --write --loglevel silent \"**/*.{json,md}\" && sort-package-json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-http-proxy": "^1.6.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "sort-package-json": "^1.44.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "vercel": "^19.1.2"
  }
}
