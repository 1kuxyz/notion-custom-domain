{
  "name": "notion-custom-domain",
  "version": "0.1.0",
  "description": "Custom Domains for Notion",
  "license": "MIT",
  "scripts": {
    "debug": "node --nolazy --inspect-brk -r ts-node/register src/index.ts",
    "deploy": "now",
    "deploy:prod": "now --prod",
    "develop": "now dev",
    "postinstall": "ts-node scripts/setup-routes.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix && sort-package-json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/express-http-proxy": "^1.5.12",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^10.0.8",
    "now": "^17.0.4",
    "prettier": "^2.0.2",
    "sort-package-json": "^1.40.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
